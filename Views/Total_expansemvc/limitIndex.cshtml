@model IEnumerable<expanseApigateway.Models.expanse_limit>

@{
    ViewBag.Title = "limitIndex";
}

<h2 style="text-align:center">Total Exapnse Limit</h2>
@Html.ActionLink("Expanselist", "Index", "expansemvc")


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Expanse_limit1)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
